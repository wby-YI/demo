带ID---修改\n    if (tradeMark.id) {\n        return request({ url: '/admin/product/baseTrademark/update', method: 'put', data: tradeMark });\n    } else {\n        //新增品牌\n        return request({ url: '/admin/product/baseTrademark/save', method: 'post', data: tradeMark });\n    }\n}\n\n//删除品牌\n///admin/product/baseTrademark/remove/{id}  delete\nexport const reqDeleteTradeMark = (id) => request({ url: `/admin/product/baseTrademark/remove/${id}`, method: 'delete' });"],"mappings":";AAAA;AACA,OAAOA,OAAP,MAAoB,iBAApB,C,CACA;AACA;;AACA,OAAO,IAAMC,gBAAgB,GAAG,SAAnBA,gBAAmB,CAACC,IAAD,EAAOC,KAAP;EAAA,OAAiBH,OAAO,CAAC;IAAEI,GAAG,yCAAkCF,IAAlC,cAA0CC,KAA1C,CAAL;IAAwDE,MAAM,EAAE;EAAhE,CAAD,CAAxB;AAAA,CAAzB,C,CAGP;AACA;AACA;AAEA;AACA;AACA;;AAEA,OAAO,IAAMC,uBAAuB,GAAG,SAA1BA,uBAA0B,CAACC,SAAD,EAAe;EAClD;EACA,IAAIA,SAAS,CAACC,EAAd,EAAkB;IACd,OAAOR,OAAO,CAAC;MAAEI,GAAG,EAAE,qCAAP;MAA8CC,MAAM,EAAE,KAAtD;MAA6DI,IAAI,EAAEF;IAAnE,CAAD,CAAd;EACH,CAFD,MAEO;IACH;IACA,OAAOP,OAAO,CAAC;MAAEI,GAAG,EAAE,mCAAP;MAA4CC,MAAM,EAAE,MAApD;MAA4DI,IAAI,EAAEF;IAAlE,CAAD,CAAd;EACH;AACJ,CARM,C,CAUP;AACA;;AACA,OAAO,IAAMG,kBAAkB,GAAG,SAArBA,kBAAqB,CAACF,EAAD;EAAA,OAAQR,OAAO,CAAC;IAAEI,GAAG,gDAAyCI,EAAzC,CAAL;IAAoDH,MAAM,EAAE;EAA5D,CAAD,CAAf;AAAA,CAA3B"}]}