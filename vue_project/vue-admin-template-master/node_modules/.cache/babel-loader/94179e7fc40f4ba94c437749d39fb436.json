})\n\n// request interceptor\n//请求拦截器：携带的token字段\nservice.interceptors.request.use(\n    config => {\n        // do something before request is sent\n\n        if (store.getters.token) {\n            // let each request carry token\n            // ['X-Token'] is a custom headers key\n            // please modify it according to the actual situation\n            config.headers['token'] = getToken()\n        }\n        return config\n    },\n    error => {\n        // do something with request error\n        console.log(error) // for debug\n        return Promise.reject(error)\n    }\n)\n\n// response interceptor\n//响应拦截器：\nservice.interceptors.response.use(\n    /**\n     * If you want to get http information such as headers or status\n     * Please return  response => response\n     */\n\n    /**\n     * Determine the request status by custom code\n     * Here is just an example\n     * You can also judge the status by HTTP Status Code\n     */\n    response => {\n        const res = response.data\n\n        // if the custom code is not 20000, it is judged as an error.\n        //服务器响应失败做什么\n        //修改一下判断，因为我们真实的服务器返回的code，可能是20000，也有可能是200\n        if (res.code !== 20000 && res.code !== 200) {\n            Message({\n                message: res.message || 'Error',\n                type: 'error',\n                duration: 5 * 1000\n            })\n\n            // 50008: Illegal token; 50012: Other clients logged in; 50014: Token expired;\n            if (res.code === 50008 || res.code === 50012 || res.code === 50014) {\n                // to re-login\n                MessageBox.confirm('You have been logged out, you can cancel to stay on this page, or log in again', 'Confirm logout', {\n                    confirmButtonText: 'Re-Login',\n                    cancelButtonText: 'Cancel',\n                    type: 'warning'\n                }).then(() => {\n                    store.dispatch('user/resetToken').then(() => {\n                        location.reload()\n                    })\n                })\n            }\n            return Promise.reject(new Error(res.message || 'Error'))\n        } else {\n            //服务器响应成功干什么\n            return res\n        }\n    },\n    error => {\n        console.log('err' + error) // for debug\n        Message({\n            message: error.message,\n            type: 'error',\n            duration: 5 * 1000\n        })\n        return Promise.reject(error)\n    }\n)\n\nexport default service"],"mappings":";AAAA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,UAAT,EAAqBC,OAArB,QAAoC,YAApC;AACA,OAAOC,KAAP,MAAkB,SAAlB;AACA,SAASC,QAAT,QAAyB,cAAzB,C,CAEA;;AACA,IAAMC,OAAO,GAAGL,KAAK,CAACM,MAAN,CAAa;EACzBC,OAAO,EAAEC,OAAO,CAACC,GAAR,CAAYC,gBADI;EACc;EACvC;EACAC,OAAO,EAAE,IAHgB,CAGX;;AAHW,CAAb,CAAhB,C,CAMA;AACA;;AACAN,OAAO,CAACO,YAAR,CAAqBC,OAArB,CAA6BC,GAA7B,CACI,UAAAC,MAAM,EAAI;EACN;EAEA,IAAIZ,KAAK,CAACa,OAAN,CAAcC,KAAlB,EAAyB;IACrB;IACA;IACA;IACAF,MAAM,CAACG,OAAP,CAAe,OAAf,IAA0Bd,QAAQ,EAAlC;EACH;;EACD,OAAOW,MAAP;AACH,CAXL,EAYI,UAAAI,KAAK,EAAI;EACL;EACAC,OAAO,CAACC,GAAR,CAAYF,KAAZ,EAFK,CAEc;;EACnB,OAAOG,OAAO,CAACC,MAAR,CAAeJ,KAAf,CAAP;AACH,CAhBL,E,CAmBA;AACA;;AACAd,OAAO,CAACO,YAAR,CAAqBY,QAArB,CAA8BV,GAA9B;AACI;AACJ;AACA;AACA;;AAEI;AACJ;AACA;AACA;AACA;AACI,UAAAU,QAAQ,EAAI;EACR,IAAMC,GAAG,GAAGD,QAAQ,CAACE,IAArB,CADQ,CAGR;EACA;EACA;;EACA,IAAID,GAAG,CAACE,IAAJ,KAAa,KAAb,IAAsBF,GAAG,CAACE,IAAJ,KAAa,GAAvC,EAA4C;IACxCzB,OAAO,CAAC;MACJ0B,OAAO,EAAEH,GAAG,CAACG,OAAJ,IAAe,OADpB;MAEJC,IAAI,EAAE,OAFF;MAGJC,QAAQ,EAAE,IAAI;IAHV,CAAD,CAAP,CADwC,CAOxC;;IACA,IAAIL,GAAG,CAACE,IAAJ,KAAa,KAAb,IAAsBF,GAAG,CAACE,IAAJ,KAAa,KAAnC,IAA4CF,GAAG,CAACE,IAAJ,KAAa,KAA7D,EAAoE;MAChE;MACA1B,UAAU,CAAC8B,OAAX,CAAmB,gFAAnB,EAAqG,gBAArG,EAAuH;QACnHC,iBAAiB,EAAE,UADgG;QAEnHC,gBAAgB,EAAE,QAFiG;QAGnHJ,IAAI,EAAE;MAH6G,CAAvH,EAIGK,IAJH,CAIQ,YAAM;QACV/B,KAAK,CAACgC,QAAN,CAAe,iBAAf,EAAkCD,IAAlC,CAAuC,YAAM;UACzCE,QAAQ,CAACC,MAAT;QACH,CAFD;MAGH,CARD;IASH;;IACD,OAAOf,OAAO,CAACC,MAAR,CAAe,IAAIe,KAAJ,CAAUb,GAAG,CAACG,OAAJ,IAAe,OAAzB,CAAf,CAAP;E